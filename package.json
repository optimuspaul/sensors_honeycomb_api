{
    "name": "honeycomb",
    "version": "1.1.4",
    "description": "wildflower data service",
    "repository": "https://github.com/WildflowerSchools/sensors_honeycomb_api",
    "main": "src/index.js",
    "scripts": {
        "test": "ENVIRONMENT=local AWS_REGION=us-east-2 AWS_ACCESS_KEY_ID=aws_access_key_id AWS_SECRET_ACCESS_KEY=aws_secret_access_key BEEHIVE_ENABLE_EVENTS=yes BEEHIVE_MOCK_STREAM=yes mocha --recursive --timeout 30000",
        "test-events": "DEBUG=yes AWS_REGION=us-east-2 BEEHIVE_ENABLE_EVENTS=yes mocha --recursive --timeout 30000",
        "coverage": "node_modules/.bin/nyc npm test",
        "start": "ENVIRONMENT=local node_modules/.bin/nodemon --watch src src/index.js"
    },
    "nyc": {
        "check-coverage": true,
        "per-file": true,
        "lines": 70,
        "statements": 70,
        "functions": 70,
        "branches": 50,
        "include": [
            "src/**/*.js"
        ],
        "exclude": [
            "src/**/*.spec.js",
            "src/schema/*.js",
            "src/test*.js",
            "src/index.js"
        ],
        "reporter": [
            "lcov",
            "text-summary"
        ],
        "cache": false,
        "all": true
    },
    "author": "pjdecoursey",
    "license": "MIT",
    "dependencies": {
        "@wildflowerschools/graphql-beehive": "^1.3.5",
        "apollo": "^1.9.2",
        "apollo-server": "^2.14.2",
        "apollo-server-express": "^2.14.2",
        "axios": ">=0.21.1",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "express": "^4.16.4",
        "express-jwt": "^6.0.0",
        "graphql": "^14.0.2",
        "graphql-custom-types": "^1.4.0",
        "graphql-directive": "^0.2.1",
        "graphql-iso-date": "^3.6.1",
        "graphql-tools": "^4.0.1",
        "graphql-voyager": "^1.0.0-rc.26",
        "js-base64": "^2.4.9",
        "jwks-rsa": "^1.3.0",
        "pg": "^7.5.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "core-js": "^3.4.1",
        "docker-run": "^3.1.0",
        "event-stream": "^3.3.4",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.4",
        "npm": "^6.14.6",
        "request": "^2.88.0",
        "nyc": "^14.1.1",
        "graphql-request": "^1.8.2",
        "kinesalite": "^3.1.0",
        "wait-port": "^0.2.2"
    }
}
